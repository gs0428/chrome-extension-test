{
  "manifest_version": 3,
  "name": "Permission",
  "description": "This extension demonstrates a 'browser_action' with permissions.",
  "version": "1.0",
  "permissions": ["sidePanel", "bookmarks", "history", "tabs"],
  "side_panel": {
    "default_path": "dist/index.html"
  },
  "background": {
    "service_worker": "service-worker.js"
  }
}
